<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:lyl="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <com.example.lyl.myapplication.customview.MyView

        android:layout_width="wrap_content"
        android:layout_height="100dp"
        lyl:bg="@mipmap/ic_launcher"
        lyl:name="lyl"
        lyl:text_size="20sp" />


    <com.example.lyl.myapplication.customview.MyViewpager
        android:id="@+id/viewpager"
        android:layout_width="wrap_content"
        android:layout_height="200dp" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.example.lyl.myapplication.customview.SlideBar
            android:layout_width="20dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:background="#f00" />
    </FrameLayout>

</RelativeLayout>
